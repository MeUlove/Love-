#include<iostream>
#include<cstdio>
#include<cstring>
#include<cmath>
#include<algorithm>
using namespace std;
int t,k;
int n,m;
long long c[2010][2010];
int ans[2010][2010];
int ans1;
int main()
{
    scanf("%d %d",&t,&k);
    c[0][0]=1;
    for(int i=1;i<=2000;i++)
    {
        c[i][0]=1;
        c[0][i]=0;
    }             
    for(int i=1;i<=2000;i++)
    {
        for(int j=1;j<=i;j++)
        {
            c[i][j]=(c[i-1][j]+c[i-1][j-1])%k; 
        }
    }       
    for(int i=0;i<=2000;i++)
    {
        for(int j=0;j<=i;j++)
        {
            if(c[i][j]%k==0)ans[i][j]=ans[i][j-1]+1;
            else ans[i][j]=ans[i][j-1];
        }
    }                                                                           
    for(int i=1;i<=t;i++)
    {
        scanf("%d %d",&n,&m);
        ans1=0;
        for(int i=0;i<=n;i++)
        {
            int x=min(i,m);
            ans1+=ans[i][x];
        }
        printf("%d\n",ans1);
        
    }
    return 0;
}
