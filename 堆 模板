#include<iostream>
#include<cstdio>
#include<cstring>
#include<cmath>
#include<algorithm>
using namespace std;
int n,a[1000010],tot;
void add1(int x)
{
	int now=++tot;
	int next=now/2;
	while(next&&a[next]>x)
	{
		a[now]=a[next];
		now=next;
		next/=2;
	}
	a[now]=x;
}
void pop1()
{
	int now=1;
	int next=now*2;
	if(next+1<tot&&a[next+1]<a[next])next++;
	while(next<tot&&a[next]<a[tot])
	{
		a[now]=a[next];
		now=next;
		next*=2;
		if(next+1<tot&&a[next+1]<a[next])next++;
	}
	a[now]=a[tot];
	tot--;
}
int main()
{
	scanf("%d",&n);
	for(int i=1;i<=n;i++)
	{
		int z;
		scanf("%d",&z);
		if(z==1)
		{
			int x;
			scanf("%d",&x);
			add1(x);
		}
		else if(z==2)printf("%d\n",a[1]);
		else pop1();
	}
	return 0;
}
