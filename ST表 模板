#include<iostream>
#include<cstdio>
#include<cstring>
#include<cmath>
#include<algorithm>
using namespace std;
int n,m;
int st[1000010][30];
int log1[1000010];
int main()
{
    scanf("%d %d",&n,&m);
    for(int i=1;i<=n;i++)log1[i]=log1[i-1]+(1<<(log1[i-1]+1)==i);
    for(int i=1;i<=n;i++)
    {
        int x;
        scanf("%d",&x);
        st[i][0]=x;
    }
    for(int j=1;(1<<j)<=n;j++)
    {
        for(int i=1;i+(1<<j)-1<=n;i++)
        {
            st[i][j]=max(st[i][j-1],st[i+(1<<(j-1))][j-1]);
        }
    }
    for(int i=1;i<=m;i++)
    {
        int l,r;
        scanf("%d %d",&l,&r);
        int k=log1[r-l+1];
        int ans=max(st[l][k],st[r-(1<<k)+1][k]);
        printf("%d\n",ans);
    }
    return 0;
}
