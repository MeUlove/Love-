#include<iostream>
#include<cstdio>
#include<cstring>
#include<cmath>
#include<algorithm>
using namespace std;
int n,m,s;
int g1[600010],tot1,g2[600010],tot2;
struct shu
{
	int to;
	int next;
}e1[600010];
struct bian
{
	int to;
	int next;
	int id;
}e2[600010];
void add1(int a,int b)
{
	tot1++;
	e1[tot1].to=b;
	e1[tot1].next=g1[a];
	g1[a]=tot1;
}
void add2(int a,int b,int c)
{
	tot2++;
	e2[tot2].to=b;
	e2[tot2].next=g2[a];
	e2[tot2].id=c;
	g2[a]=tot2;
}
int pre[600010];
int find(int x)
{
	if(pre[x]==x)return x;
	else return pre[x]=find(pre[x]);
}
int v[600010],vis[600010];
int ans[600010];
void lca(int a)
{
	v[a]=1;
	int tmp=g1[a];
	while(tmp!=0)
	{
		if(v[e1[tmp].to]==0)
		{
			lca(e1[tmp].to);
			pre[e1[tmp].to]=a;
		}
		tmp=e1[tmp].next;
	}
	vis[a]=1;
	int temp=g2[a];
	while(temp!=0)
	{
		if(vis[e2[temp].to]==1)
		{
			ans[e2[temp].id]=find(e2[temp].to);     
			//当前节点为a，由于e2[temp].to已经访问过，
			//于是在求公共祖先时，需要寻找的是两点间最先访问过的点的祖宗 
		}
		temp=e2[temp].next;
	}
}
int main()
{
	scanf("%d %d %d",&n,&m,&s);
	for(int i=1;i<=n;i++)pre[i]=i;         //初始化 
	for(int i=1;i<=n-1;i++)
	{
		int x,y;
		scanf("%d %d",&x,&y);
		add1(x,y);
		add1(y,x);
	}
	for(int i=1;i<=m;i++)
	{
		int x,y;
		scanf("%d %d",&x,&y);
		add2(x,y,i);
		add2(y,x,i);
	}
	lca(s);
	for(int i=1;i<=m;i++)printf("%d\n",ans[i]);    
	return 0;
}
